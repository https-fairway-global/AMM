{
  "name": "@identity-amm/signature-registry-contract",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "author": "Your Name/Organization",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "compact": "compactc --idl --ts --out-dir ./src/generated ./src/registry.compact",
    "test": "jest",
    "prepack": "yarn build",
    "build": "rm -rf dist && yarn compact && tsc -b tsconfig.build.json && cp ./src/registry.compact ./dist",
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@midnight-ntwrk/compact-compiler": "workspace:*",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@midnight-ntwrk/compact-runtime": "workspace:*"
  }
} 